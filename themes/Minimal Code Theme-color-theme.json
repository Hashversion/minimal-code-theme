{
  "name": "Minimal Code Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // Variable ---------------------------
    "variable": "#e3e3e3",

    // Number ------------------------------
    "number": "#00c3ff",

    // "boolean": "#bf5959",

    // Enums -----------------------------
    // Enum variable name
    "enum": "#cccccc",
    // Enum children
    "enumMember": {
      "foreground": "#96e2ee"
    },

    // "type": "#919191",

    // Macros ------------------------------
    "macro": {
      "foreground": "#6feff8",
      "underline": true
    },
    "macroBang": "#8a5ff1",

    "function.defaultLibrary": "#a390ff",
    "class.defaultLibrary": "#a390ff"
  },
  "colors": {
    // General
    "foreground": "#BFD4E1",
    "focusBorder": "#2a2a2a",
    "progressBar.background": "#8250df",
    "selection.background": "#6f6f6f",
    "scrollbar.shadow": "#484f5833",

    // Activity Bar
    "activityBar.foreground": "#a475f9",
    "activityBar.background": "#0d1117",
    "activityBar.inactiveForeground": "#999",
    "activityBarBadge.foreground": "#000",
    "activityBarBadge.background": "#a475f9",
    "activityBar.border": "#30363d",
    "activityBar.activeBackground": "#000000",

    // Side Bar
    "sideBar.background": "#000",
    "sideBar.foreground": "#c9d1d9",
    "sideBarSectionHeader.background": "#010409",
    "sideBarSectionHeader.border": "#30363d",
    "sideBarSectionHeader.foreground": "#c5c5c5",
    "sideBarTitle.foreground": "#c5c5c5",
    "sideBar.border": "#1a1a1a",

    // List from Side Bar
    "list.inactiveSelectionBackground": "#6e768166",
    "list.activeSelectionBackground": "#6e768166",
    "list.hoverBackground": "#151515",
    "list.hoverForeground": "#ffffff",
    "list.dropBackground": "#151515",

    // Tree from Side Bar
    "tree.indentGuidesStroke": "#5c5c5c",
    "tree.inactiveIndentGuidesStroke": "#5c5c5c",

    // List from Command Pallet
    "list.highlightForeground": "#a475f9",
    "list.focusBackground": "#e5e5e51a",
    "list.focusForeground": "#505050",
    "list.focusOutline": "#e5e5e51a",

    // Dropdown
    "dropdown.background": "#161b22",
    "dropdown.border": "#30363d",
    "dropdown.foreground": "#c9d1d9",
    "dropdown.listBackground": "#161b22",

    // Status Bar
    "statusBar.background": "#000000",
    "statusBar.foreground": "#808080",
    "statusBar.border": "#00000000",
    "statusBar.debuggingBackground": "#bc4c00",
    "statusBar.debuggingForeground": "#999",
    "statusBar.noFolderBackground": "#000000",
    "statusBar.noFolderForeground": "#e5e5e5",
    "statusBarItem.remoteBackground": "#a475f9",
    "statusBarItem.remoteForeground": "#151515",

    // Title Bar
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#e9e9e9",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#bbbbbb",
    "titleBar.border": "#00000000",

    // Menu & Menu Bar
    "menu.border": "#30363d",

    // Buttons
    "button.background": "#AD9CFF",
    "button.foreground": "#000000",
    "button.hoverBackground": "#a475f9",
    "textLink.foreground": "#AD9CFF",
    "button.secondaryForeground": "#c5c5c5",
    "button.secondaryHoverBackground": "#373737",

    // Inputs
    "input.background": "#0d1117",
    "input.foreground": "#999",
    "input.border": "#30363d",
    "input.placeholderForeground": "#6e7681",

    "tab.activeBackground": "#151515",
    "tab.activeForeground": "#e5e5e5",
    "tab.inactiveBackground": "#000",
    "tab.inactiveForeground": "#999",
    "tab.hoverBackground": "#E5E5E51A",
    "tab.border": "#0c0c0c",

    "editor.background": "#0d1117",
    "editorLineNumber.foreground": "#858585",
    "editorLineNumber.activeForeground": "#c6c6c6",
    "editorSuggestWidget.background": "#000000",
    "editorSuggestWidget.border": "#363636",
    "editorSuggestWidget.foreground": "#e2e2e2",
    "editorSuggestWidget.highlightForeground": "#AD9CFF",
    "editorSuggestWidget.selectedForeground": "#000000",
    "editorSuggestWidget.selectedBackground": "#a28dec",

    "breadcrumb.background": "#151515",
    "breadcrumb.foreground": "#999",
    "breadcrumb.focusForeground": "#e5e5e5",
    "editorGroupHeader.border": "#00000000",
    "editorGroupHeader.tabsBackground": "#000",

    "scrollbarSlider.background": "#79797966",
    "scrollbarSlider.hoverBackground": "#646464b3",
    "scrollbarSlider.activeBackground": "#bfbfbf66",

    "widget.shadow": "#0000005c",
    "editorWidget.foreground": "#ccc",
    "editorWidget.background": "#252526",
    "pickerGroup.border": "#3f3f46",
    "pickerGroup.foreground": "#3794ff",
    "editorWidget.resizeBorder": "#5f5f5f",
    "debugToolBar.background": "#333",
    "debugToolBar.border": "#474747",

    "errorForeground": "#f48771",
    "editorError.foreground": "#f48771",
    "list.errorForeground": "#f48771",
    "list.warningForeground": "#F7CC66",
    "editorWarning.foreground": "#F7CC66",

    "gitDecoration.deletedResourceForeground": "#C54444",
    "gitDecoration.untrackedResourceForeground": "#9FE7A0",
    "gitDecoration.conflictingResourceForeground": "#ED6C6C",
    "gitDecoration.ignoredResourceForeground": "#585858",
    "gitDecoration.modifiedResourceForeground": "#DD763C"
  },
